<view class="analyse-page">
  <view class="analyse-safe-top" style="height: {{safeTop}}px;"></view>
  <view class="analyse-header-bar">
    <span class="icon--return icon-size-70 analyse-back-button" bindtap="onBackTap" />
  </view>
  <view class="analyse-section">
    <view class="analyse-section-title">概览</view>
    <view class="analyse-cards">
      <view class="analyse-card">
        <view class="analyse-card-label">报价单数量</view>
        <view class="analyse-card-value">{{list.length}}</view>
      </view>
    </view>
  </view>
  <view class="analyse-section">
    <view class="analyse-section-title">报价单</view>
    <block wx:if="{{loading}}">
      <view class="analyse-loading">
        <view class="icon--loading icon-size-30">⏳</view>
        <view class="analyse-loading-text">数据加载中...</view>
      </view>
    </block>
    <block wx:elif="{{list.length > 0}}">
      <analyse-quote-card wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="quoteId" item="{{item}}" index="{{index}}" bindonline="onOnlineTap" bindoffline="onOfflineTap" binddelete="onDeleteTap"></analyse-quote-card>
    </block>
    <view wx:else class="analyse-empty">
      <view class="analyse-empty-title">暂无数据</view>
      <view class="analyse-empty-desc">分享报价单后，将在这里看到分析记录</view>
    </view>
  </view>
</view>