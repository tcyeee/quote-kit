<view class="analyse-page">
  <view class="analyse-safe-top" style="height: {{safeTop}}px;"></view>
  <view class="analyse-header-bar">
    <span class="icon--return icon-size-70 analyse-back-button" bindtap="onBackTap" />
  </view>
  <view class="analyse-cards">
    <view class="analyse-card">
      <view class="analyse-card-label">报价单数量</view>
      <view class="analyse-card-value">{{list.length}}</view>
    </view>
  </view>
  <view class="analyse-section">
    <view class="analyse-section-title">报价单</view>
    <view wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="quoteId" class="analyse-category-row">
      <view class="analyse-category-name">{{item.quote.clientName}}</view>
      <view class="analyse-category-meta">
        <view class="analyse-category-count">{{item.quote.computeData.totalAmount}} 元</view>
        <view>分享时间：{{item.shareTimeText}}</view>
        <view>截止时间：{{item.expireTimeText}}</view>
        <view>查看次数：{{item.viewCountDisplay}}</view>
      </view>
    </view>
  </view>
</view>