<view class="analyse-page">
  <view class="analyse-safe-top" style="height: {{safeTop}}px;"></view>
  <view class="analyse-header-bar">
    <span class="icon--return icon-size-70 analyse-back-button" bindtap="onBackTap" />
  </view>
  <view class="analyse-cards">
    <view class="analyse-card">
      <view class="analyse-card-label">报价单数量</view>
      <view class="analyse-card-value">{{list.length}}</view>
    </view>
  </view>
  <view class="analyse-section">
    <view class="analyse-section-title">报价单</view>
    <analyse-quote-card wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="quoteId" item="{{item}}" index="{{index}}" bindoffline="onOfflineTap" binddelete="onDeleteTap"></analyse-quote-card>
  </view>
</view>