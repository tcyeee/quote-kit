.config-schema-header {
  display: flex;
  gap: 7rpx;
  background-color: #c0a28a43;
  padding: 5rpx 7rpx;
  border-radius: 10rpx;
  margin-bottom: 12rpx;
}

.config-schema-section + .config-schema-section {
  margin-top: 24rpx;
  padding-top: 20rpx;
  border-top: 2rpx dashed #e0d7ce;
}

.config-schema-title {
  font-size: 26rpx;
  font-weight: 600;
  color: #6b665f;
  line-height: 20px;
}

.config-schema-group {
  display: flex;
  flex-direction: column;
  gap: 12rpx;
}

